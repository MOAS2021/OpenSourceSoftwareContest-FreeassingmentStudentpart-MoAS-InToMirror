<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/register"
    tools:context=".RegisterActivity">

    <ImageButton
        android:id="@+id/imageButton1R"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="75dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/a1" />

    <ImageButton
        android:id="@+id/imageButton2R"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/imageButton1R"
        app:layout_constraintTop_toTopOf="@+id/imageButton1R"
        app:srcCompat="@drawable/a2" />

    <ImageButton
        android:id="@+id/imageButton3R"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/imageButton2R"
        app:layout_constraintTop_toTopOf="@+id/imageButton2R"
        app:srcCompat="@drawable/a3" />

    <ImageButton
        android:id="@+id/imageButton4R"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton3R"
        app:layout_constraintTop_toTopOf="@+id/imageButton3R"
        app:srcCompat="@drawable/a4" />

</androidx.constraintlayout.widget.ConstraintLayout>



